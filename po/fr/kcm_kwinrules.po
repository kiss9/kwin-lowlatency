#: optionsmodel.cpp:145
#, kde-format
msgid "Unimportant"
msgstr "Sans importance"

#: optionsmodel.cpp:146
#, kde-format
msgid "Exact Match"
msgstr "Correspondance exacte"

#: optionsmodel.cpp:147
